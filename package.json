        "start:dev": "npm run clean && npm run build && npm run start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "npm run build"
    },
    "dependencies": {
        "assert": "^2.0.0",
        "mongodb": "^3.2.7"
    },
    "devDependencies": {
        "@types/mongodb": "^3.1.28",
        "dotenv": "^7.0.0",
        "npm-run-all": "^4.1.5",
        "typescript": "^3.4.5"
    }
}
