{
    "name": "sp-customer-utility",
    "version": "1.0.0",
    "description": "Imports customers data from prod mongo to stage mongo",
    "author": "Alan Anderson",
    "license": "ISC",
    "bin": "./bin/main.js",
    "files": [
        "package.json",
        "bin/"
    ],
    "scripts": {
        "build": "echo building... && npm-run-all build:* && echo done!",
        "build:ts": "tsc",
        "clean": "rm -rf bin/",
